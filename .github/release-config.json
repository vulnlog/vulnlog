{
  "template": "# üéâ Release {{RELEASE_TAG}}\n\n{{#if RELEASE_DESCRIPTION}}{{RELEASE_DESCRIPTION}}\n\n{{/if}}{{#if HIGHLIGHTS}}\n## üåü Highlights\n{{#each HIGHLIGHTS}}\n- {{this}}\n{{/each}}\n{{/if}}\n\n{{#if BREAKING}}\n## ‚ö†Ô∏è Breaking Changes\n{{#each BREAKING}}\n- {{this}}\n{{/each}}\n{{/if}}\n\n#{{CHANGELOG}}\n\n---\n\n{{#if CONTRIBUTORS}}\n### üôè Thanks to our contributors\n{{#each CONTRIBUTORS}}\n- @{{this}}\n{{/each}}\n{{/if}}\n\n{{#if COMPARE_URL}}\n[Full diff]({{COMPARE_URL}})\n{{/if}}\n",
  "categories": [
    {
      "title": "## ‚ú® Features",
      "labels": [
        "feat",
        "feature"
      ]
    },
    {
      "title": "## üõ† Improvements",
      "labels": [
        "perf",
        "refactor",
        "style"
      ]
    },
    {
      "title": "## üêõ Bug Fixes",
      "labels": [
        "fix",
        "bug"
      ]
    },
    {
      "title": "## üìö Documentation",
      "labels": [
        "docs"
      ]
    },
    {
      "title": "## üö¶ CI/CD",
      "labels": [
        "ci"
      ]
    },
    {
      "title": "## üß± Build",
      "labels": [
        "build"
      ]
    },
    {
      "title": "## ‚úÖ Tests",
      "labels": [
        "test"
      ]
    },
    {
      "title": "## üîÅ Other",
      "labels": [
        "chore",
        "revert"
      ]
    }
  ],
  "label_extractor": [
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test){1}(\\(([\\w\\-\\.]+)\\))?(!)?:\\s(.+)$",
      "on_property": "title",
      "target": "$1"
    }
  ],
  "transformer": {
    "pr_template": "- {{TITLE}}{{#if SCOPE}} ({{SCOPE}}){{/if}} by @{{AUTHOR}} in {{PULL_REQUEST}}",
    "issue_template": "- {{TITLE}} in {{ISSUE}}"
  },
  "pr_template": "- {{TITLE}} by @{{AUTHOR}} in {{PULL_REQUEST}}",
  "empty_template": "_No changes in this release._",
  "sort": {
    "order": [
      "Features",
      "Improvements",
      "Bug Fixes",
      "Documentation",
      "CI/CD",
      "Build",
      "Tests",
      "Other"
    ]
  }
}
