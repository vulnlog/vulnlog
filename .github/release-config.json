{
  "template": "#{{CHANGELOG}}",
  "categories": [
    {
      "title": "## Feature",
      "labels": [
        "feat",
        "feature"
      ]
    },
    {
      "title": "## Fix",
      "labels": [
        "fix",
        "bug"
      ]
    },
    {
      "title": "## Other",
      "labels": []
    }
  ],
  "label_extractor": [
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test){1}(\\([\\w\\-\\.]+\\))?(!)?: ([\\w ])+([\\s\\S]*)",
      "on_property": "title",
      "target": "$1"
    }
  ]
}
